{
  "name": "@dreamstack/kaal-themes",
  "version": "0.1.0",
  "description": "Theme system for @dreamstack/kaal",
  "type": "module",
  "main": "./lib/module/index.js",
  "types": "./lib/typescript/src/index.d.ts",
  "source": "./src/index.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "types": "./lib/typescript/src/index.d.ts",
      "default": "./lib/module/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": ["lib", "src"],
  "scripts": {
    "build": "bob build",
    "dev": "bob build --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf lib"
  },
  "peerDependencies": {
    "react-native-unistyles": "^3.0.0"
  },
  "devDependencies": {
    "react-native-builder-bob": "^0.35.0",
    "react-native-unistyles": "^3.0.0",
    "typescript": "^5.7.0",
    "typescript-config": "workspace:*"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      ["module", { "esm": true }],
      "typescript"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT"
}
