{
  "name": "@dreamstack-us/kaal-themes",
  "version": "1.0.0",
  "description": "Theme system for @dreamstack-us/kaal",
  "type": "module",
  "main": "./lib/module/index.js",
  "types": "./lib/typescript/index.d.ts",
  "source": "./src/index.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "types": "./lib/typescript/index.d.ts",
      "default": "./lib/module/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": ["lib", "src"],
  "scripts": {
    "build": "bob build",
    "dev": "bob build --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf lib"
  },
  "peerDependencies": {
    "react-native-unistyles": "^3.0.0"
  },
  "devDependencies": {
    "react-native-builder-bob": "^0.35.0",
    "react-native-unistyles": "^3.0.0",
    "typescript": "^5.7.0",
    "typescript-config": "workspace:*"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "module",
        {
          "esm": true
        }
      ],
      "typescript"
    ]
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DreamStack-us/kaal.git",
    "directory": "packages/themes"
  },
  "license": "MIT"
}
